{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"strict": true,
		"noImplicitAny": false,
        "noEmit": true,
        "target": "esnext",
        "lib": ["esnext"],
        "types": ["@cloudflare/workers-types"],
        "moduleResolution": "node"
	},
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./node_modules",
        "./src/**/*.test.js"
    ]
}
